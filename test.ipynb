    "from datetime import datetime\n",
    "\n",
    "start_date = datetime.strptime('2021-03-01', '%Y-%m-%d').date()\n",
    "end_date = datetime.strptime('2023-04-30', '%Y-%m-%d').date()\n",
    "\n",
    "num_dates = (end_date - start_date).days + 1\n",
    "\n",
    "print(num_dates)"
